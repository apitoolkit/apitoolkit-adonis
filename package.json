{
  "name": "apitoolkit-adonis",
  "version": "3.1.3",
  "description": "APIToolkit SDK for adonisjs",
  "type": "module",
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./apitoolkit_provider": "./build/providers/apitoolkit_provider.js",
    "./apitoolkit_middleware": "./build/src/middleware/apitoolkit_middleware.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint . --ext=.ts",
    "test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"stubs/**/*.stub\" build",
    "copy:instructions_md": "copyfiles \"instructions.md\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "version": "git add -A ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apitoolkit/apitoolkit-adonis.git"
  },
  "author": "",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "peerDependencies": {
    "@adonisjs/core": "^6.9.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/tsconfig": "^1.3.0",
    "@swc/core": "^1.5.7",
    "@types/jest": "^29.5.11",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/sync-fetch": "^0.4.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.55.0",
    "mrm": "^4.1.22",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "build/providers",
    "build/src",
    "build/stubs",
    "build/index.d.ts",
    "build/index.js",
    "build/configure.d.ts",
    "build/configure.js"
  ],
  "dependencies": {
    "@google-cloud/pubsub": "^4.0.7",
    "@japa/assert": "^2.1.0",
    "@japa/runner": "^3.1.1",
    "@japa/spec-reporter": "^1.3.3",
    "@poppinss/dev-utils": "^2.0.3",
    "@poppinss/utils": "^6.7.3",
    "apitoolkit-js": "^3.3.2",
    "async_hooks": "^1.0.0",
    "axios": "^1.6.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "jsonpath": "^1.1.1",
    "reflect-metadata": "^0.2.1",
    "supertest": "^6.3.3",
    "sync-fetch": "^0.5.2",
    "uuid": "^9.0.1"
  }
}
